{
  "name": "Chrome Ext",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome Ext Template",
  "permissions": ["identity", "identity.email", "tabs", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "192": "logo192.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "web_accessible_resources": [],
  "browser_action": {
    "default_icon": "logo192.png",
    "default_popup": "popup.html"
  }
}
